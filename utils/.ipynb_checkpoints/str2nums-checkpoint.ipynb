   "source": [
    "from __future__ import absolute_import\n",
    "\n",
    "\n",
    "def chars2nums(a):\n",
    "    nums_list = ['0']\n",
    "    for k in a:\n",
    "        # print(k)\n",
    "        if k == ',':\n",
    "            nums_list.append('0')\n",
    "        else:\n",
    "            nums_list[-1] = nums_list[-1] + k\n",
    "\n",
    "    return [int(char_) for char_ in nums_list]\n",
    "    \n"
   ]
  }
